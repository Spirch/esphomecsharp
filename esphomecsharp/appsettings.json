{
  "Settings": {
    "DBFileName": "s31-outlet-sqlite.db",
    "TotalInsertInterval": 30,
    "ShowErrorInterval": 10,
    "DateTimeFormat": "yyyy-MM-dd HH:mm:ss",
    "Pages": {
      "1": "s31-outlet"
    }
