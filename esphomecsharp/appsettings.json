  "s31-outlet-RowInfo": [
    {
      "Prefix": "switch-",
      "Suffix": "",
      "Name": "Status",
      "Unit": "",
      "Column": 1
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_wifi_signal",
      "Name": "Wifi",
      "Unit": "dBm",
      "Column": 2,
      "Width": 12,
      "RecordDelta": 2,
      "RecordThrottle": 60
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_daily_energy",
      "Name": "Daily",
      "Unit": "kW",
      "Column": 3,
      "Width": 12,
      "IsTotalDailyEnergy": true,
      "RecordDelta": 0.1,
      "RecordThrottle": 60
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_voltage",
      "Name": "Voltage",
      "Unit": "V",
      "Column": 4,
      "Width": 12,
      "RecordDelta": 1.25,
      "RecordThrottle": 60
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_current",
      "Name": "Current",
      "Unit": "A",
      "Column": 5,
      "Width": 12,
      "RecordDelta": 0.15,
      "RecordThrottle": 60
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_power",
      "Name": "Power",
      "Unit": "W",
      "Column": 6,
      "Width": 12,
      "IsTotalPower": true,
      "RecordDelta": 75,
      "RecordThrottle": 60
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_total_power",
      "Name": "TPower",
      "Unit": "kWh",
      "Column": 7,
      "Width": 12,
      "RecordDelta": 0.1,
      "RecordThrottle": 60
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_apparent_power",
      "Name": "APower",
      "Unit": "VA",
      "Column": 8,
      "Width": 12,
      "RecordDelta": 75,
      "RecordThrottle": 60
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_power_factor",
      "Name": "FPower",
      "Unit": "",
      "Column": 9,
      "RecordDelta": 0.15,
      "RecordThrottle": 60
    },
    {
      "Prefix": "event-",
      "Suffix": "_button",
      "Name": "Pressed",
      "Unit": "",
      "Hidden": true
    }
  ],
