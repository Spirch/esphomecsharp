      "Unit": "W",
      "Column": 6,
      "IsTotalPower": true,
      "RecordDelta": 5,
      "RecordThrottle": 30
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_total_power",
      "Name": "Total Power",
      "Unit": "kWh",
      "Column": 7,
      "IsTotalPower": false,
      "RecordDelta": 0.05,
      "RecordThrottle": 30
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_apparent_power",
      "Name": "Apparent Power",
      "Unit": "VA",
      "Column": 8,
      "IsTotalPower": false,
      "RecordDelta": 0.05,
      "RecordThrottle": 30
    },
    {
      "Prefix": "sensor-",
      "Suffix": "_power_factor",
      "Name": "Power Factor",
      "Unit": "",
      "Column": 9,
      "IsTotalPower": false,
      "RecordDelta": 0.05,
      "RecordThrottle": 30
    }
  ],
